@model IEnumerable<SurveyPage.Models.Survey>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SurveyName)
        </th>
        <th></th>
    </tr>

    @foreach (var survey in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => survey.SurveyName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = survey.Id }) |
                @Html.ActionLink("Details", "Details", new { id = survey.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = survey.Id })
            </td>
        </tr>
    }


</table>